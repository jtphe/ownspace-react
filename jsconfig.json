{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2017",
    "baseUrl": "./",
    "module": "commonjs",
    "paths": {
      "@components/*": ["src/components/*"],
      "@router/*": ["src/config/*"],
      "@utils/*": ["src/utils/*"],
      "@shared/*": ["src/shared/*"],
      "@store/*": ["src/store/*"],
      "@backend/*": ["backend/backend/*"],
      "@api/*": ["src/api/*"],
      "@images/*": ["src/images/*"],
      "@i18n/*": ["src/i18n/*"],
      "@constants/*": ["src/constants/*"]
    }
  },
  "exclude": ["nodes_modules", "assets"]
}
